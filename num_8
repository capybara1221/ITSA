#include <stdio.h>
#include <string.h>
#include <math.h>
#include <ctype.h>

// 8
// C_AR63: 矩陣相加
int main(void)
{
    int r1,c1,r2,c2;
    
    scanf("%d %d",&r1,&c1);
    
    int m1[r1][c1];
    for(int i=0;i<r1;i++)
    {
        scanf("%*[\n]");
        for(int j=0;j<c1;j++)
        {
            scanf("%d",&m1[i][j]);
        }
    }
    
    scanf("%d %d",&r2,&c2);
    
    int m2[r2][c2];
    for(int i=0;i<r2;i++)
    {
        scanf("%*[\n]");
        for(int j=0;j<c2;j++)
        {
            scanf("%d",&m2[i][j]);
        }
    }
    
    int out[r1][c1];
    //memset(out,0,sizeof(int)*r1*r2);
    
    for(int i=0;i<r1;i++)
    {
        for(int j=0;j<c2;j++)
        {
            out[i][j]=0;
        }
    }
    
    for(int i=0;i<r1;i++)
    {
        for(int j=0;j<c1;j++)
        {
            out[i][j]=m1[i][j]+m2[i][j];
        }
    }
    
    for(int i=0;i<r1;i++)
    {
        for(int j=0;j<c1;j++)
        {
            printf("%d",out[i][j]);
            if(j!=c1-1)
                printf(" ");
        }
        printf("\n");
    }
}
