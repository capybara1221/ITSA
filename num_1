#include <stdio.h>
#include <string.h>
#include <math.h>
#include <ctype.h>

// 1
// C_AR03: 計算陣列中所有元素的立方和
int main(void)
{
    int in[6];
    
    scanf("%d %d %d %d %d %d",&in[0],&in[1],&in[2],&in[3],&in[4],&in[5]);
    
    printf("%d\n",in[0]*in[0]*in[0]+in[1]*in[1]*in[1]+in[2]*in[2]*in[2]+in[3]*in[3]*in[3]+in[4]*in[4]*in[4]+in[5]*in[5]*in[5]);
    
}
